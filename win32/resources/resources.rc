      VALUE "OriginalFilename", "busybox.exe"
      VALUE "ProductName", "busybox-w32"
      VALUE "ProductVersion", BB_VER
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x809, 1252
  END
END
