#include <autoconf.h>

#if ENABLE_FEATURE_ICON_ATERM || ENABLE_FEATURE_ICON_ALL
1 ICON "aterm.ico"
#endif
#if ENABLE_FEATURE_ICON_STERM || ENABLE_FEATURE_ICON_ALL
2 ICON "sterm.ico"
#endif

